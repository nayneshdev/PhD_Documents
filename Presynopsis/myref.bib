@article{Bhavin2009a,
  author={Bhavin Patel and P C Vinodkumar},
  title={Properties of ##IMG## [http://ej.iop.org/images/0954-3899/36/3/035003/toc_jpg297565ieqn1.gif] {Q\skew3\bar{Q} (Q\, {\epsilon}\, b\hbox{, }c)} mesons in Coulomb plus power potential (CPP ν )},
  journal={Journal of Physics G: Nuclear and Particle Physics},
  volume={36},
  number={3},
  pages={035003},
  url={http://stacks.iop.org/0954-3899/36/i=3/a=035003},
  year={2009},
  abstract={The decay rates and spectroscopy of the ##IMG## [http://ej.iop.org/images/0954-3899/36/3/035003/jpg297565ieqn3.gif] {Q \skew3\bar{Q} (Q \in c, b)} mesons are computed in nonrelativistic phenomenological quark–antiquark potential of the type ##IMG## [http://ej.iop.org/images/0954-3899/36/3/035003/jpg297565ieqn4.gif] {V(r)=-\frac{4}{3}\frac{\alpha_s}{r}+A r^{\nu}} with different choices ν. The numerical solution of the Schrödinger equation has been used to obtain the spectroscopy of ##IMG## [http://ej.iop.org/images/0954-3899/36/3/035003/jpg297565ieqn5.gif] {Q\skew3\bar{Q}} mesons. The spin hyperfine, spin–orbit and tensor components of the one-gluon exchange interaction are employed to compute the spectroscopy of the excited S states, few low-lying P -waves and D -waves. The numerically obtained radial solutions are employed to obtain the decay constant, digamma and dileptonic decay widths. It has been observed that the predictions of the spectroscopy and the decay widths are consistent with other model predictions as well as with the known experimental values in the range of potential index 0.7 ≤ ν ≤ 1.1.}
}

